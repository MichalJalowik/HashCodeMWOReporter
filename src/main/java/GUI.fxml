<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="300.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="GUIController">
   <left>
      <AnchorPane prefHeight="300.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="importPathField" layoutX="50.0" layoutY="33.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
            <Button id="importButton" layoutX="471.0" layoutY="74.0" mnemonicParsing="false" onMouseClicked="#importData" text="Import data" />
            <CheckBox fx:id="raport1checkbox" layoutX="50.0" layoutY="99.0" mnemonicParsing="false" text="Raport 1">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="raport2checkbox" layoutX="50.0" layoutY="126.0" mnemonicParsing="false" text="Raport 2">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="raport3checkbox" layoutX="50.0" layoutY="153.0" mnemonicParsing="false" text="Raport 3">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="raport4checkbox" layoutX="50.0" layoutY="180.0" mnemonicParsing="false" text="Raport 4">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="csvFileCheckbox" layoutX="142.0" layoutY="99.0" mnemonicParsing="false" text="*.csv">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox layoutX="142.0" layoutY="126.0" mnemonicParsing="false" text="*.xlsx" fx:id="xlsxFileCheckbox">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="jpgFileCheckbox" layoutX="142.0" layoutY="153.0" mnemonicParsing="false" text="*.jpg">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="pdfFileCheckbox" layoutX="142.0" layoutY="180.0" mnemonicParsing="false" text="*.pdf">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </CheckBox>
            <Button id="generateRaportButton" layoutX="50.0" layoutY="218.0" mnemonicParsing="false" onMouseClicked="#generateRaports" text="Generate raports" />
            <Label fx:id="textMessageLabel" layoutX="251.0" layoutY="131.0" prefHeight="110.0" prefWidth="238.0" text="Label" />
         </children>
      </AnchorPane>
   </left>
</BorderPane>
